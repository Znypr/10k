RewriteEngine On
RewriteBase /

# 1. If the requested file is a real file (like style.css or an image), serve it.
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# 2. Handle the "contact/impressum" pages
RewriteRule ^contact/(.*)$ contact/$1.html [L]

# 3. Everything else goes to index.html (The App)
RewriteRule ^ index.html [L]